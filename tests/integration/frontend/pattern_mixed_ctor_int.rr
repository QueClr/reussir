// RUN: %reussir-elab --mode semi %s | %FileCheck %s

enum Result {
    Ok(i32),
    Err(i32)
}

fn test(x : Result) -> i32 {
    match x {
        Result::Ok(0) => 1,
        Result::Ok(n) => n,
        Result::Err(0) => 2,
        Result::Err(_) => 3
    }
}

